{
  "name": "@VijayAditya1609/fdatracker-frontend",
  "version": "0.1.1-dev.202504020935",
  "type": "module",
  "description": "FDA Tracker Frontend Application for monitoring FDA inspections and compliance",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VijayAditya1609/fdatracker_frontend.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT",
  "keywords": [
    "fda",
    "tracker",
    "compliance",
    "inspections",
    "form483",
    "warning-letters",
    "react",
    "typescript"
  ],
  "author": "VijayAditya1609",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --config vite.lib.config.ts",
    "preview": "vite preview",
    "serve": "serve -s dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:complexity": "eslint . --ext .js,.jsx,.ts,.tsx --rule 'complexity: [error, 10]' --rule 'sonarjs/cognitive-complexity: [error, 15]'",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,css,scss}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check:coverage": "vitest run --coverage --min-coverage=80",
    "type-check": "tsc --noEmit",
    "lint:duplication": "jscpd . --threshold 3",
    "a11y": "npx axe-core-cli http://localhost:3000 --exit",
    "a11y:react": "echo \"import { axe } from '@axe-core/react'; // Add to your test files\"",
    "security": "audit-ci --high",
    "security:licenses": "license-checker --production --onlyAllow=\"MIT;ISC;Apache-2.0;BSD-2-Clause;BSD-3-Clause\"",
    "bundlesize": "bundlesize",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^1.7.18",
    "@mui/material": "^6.4.4",
    "@stripe/stripe-js": "^5.6.0",
    "@types/react-modal": "^3.16.3",
    "axios": "^1.7.9",
    "chart.js": "^4.4.8",
    "date-fns": "^3.3.1",
    "framer-motion": "^12.4.7",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "node": "^18.20.6",
    "ol": "^8.2.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-hot-toast": "^2.5.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.8.1",
    "react-modal": "^3.16.3",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.15.1",
    "tailwind-scrollbar": "^3.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.2",
    "@axe-core/react": "^4.8.2",
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.2.1",
    "audit-ci": "^6.6.1",
    "autoprefixer": "^10.4.18",
    "bundlesize": "^0.18.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-sonarjs": "^0.24.0",
    "globals": "^15.9.0",
    "husky": "^9.0.0",
    "jscpd": "^3.5.10",
    "jsdom": "^24.0.0",
    "license-checker": "^25.0.1",
    "lighthouse": "^11.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.0",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  }
}
